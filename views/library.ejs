<%- include("partials/header") -%>

<h1>Library</h1>

<p> <%= libraryContent %> </p>

<% posts.forEach(function(post) { %>
<h1> <%= post.title %> </h1>
<p>
  <%= post.content.substring(0, 3000) + " ..." %>
  <a href="/posts/<%=post.title%>">Read More</a>
</p>
<p>Article Source: <%= post.source%> </p>
<% }) %>

<%- include("partials/footer") -%>
